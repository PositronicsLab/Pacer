    <error-feedback>
      <active type="bool">true</active>
      <configuration-space>
        <active type="bool">true</active>
        <accel type="bool">false</accel>
        <!-- Gains are for the q of joint ids respectively -->
        <gains>
          <kp type="double">1e1 1e1 1e1      1e1 1e1 1e1       1e1 1e2 1e3 1e1 1e1        1e1 1e2 1e3 1e1 1e1</kp>
          <kv type="double">1e-1 1e-1 1e-1   1e-1 1e-1 1e-1    1e-1 1e0 1e1 1e-1 1e-1     1e-1 1e0 1e1 1e-1 1e-1</kv>
