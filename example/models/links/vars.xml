       1.1000   -0.0952
       1.1500   -0.0994
       1.2000   -0.0996
       1.2500   -0.0959
       1.3000   -0.0883
       1.3500   -0.0773
       1.4000   -0.0631
       1.4500   -0.0465
       1.5000   -0.0279
       1.5500   -0.0083
       1.6000    0.0117
