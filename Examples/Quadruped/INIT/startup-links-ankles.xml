<XML>
  <quadruped>
    <init>
      <base>
        <active type="bool">true</active>
        <x type="double">0 0 0.36 0 -1.56 0</x>
      </base>

      <joint>
      <id type="string">0LF_X_1 0LF_Y_2 0LF_Y_3  0RF_X_1 0RF_Y_2 0RF_Y_3   0LH_X_1 0LH_Y_2 0LH_Y_3 0LH_ZY_4 1LH_ZY_4   0RH_X_1 0RH_Y_2 0RH_Y_3 0RH_ZY_4 1RH_ZY_4</id>
       <q type="double">0.3927 2.3562 -1.5708           -0.3927 -2.3562 1.5708         0 -0.7854 1.5708 0 -0.7         0 0.7854 -1.5708 0 -0.7</q>
